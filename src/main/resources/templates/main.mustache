
<!DOCTYPE html>
<html>
<head>
    <title>Person</title>
    <link rel="stylesheet" href="css/style.css" />
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>
<body>
<h2>List</h2>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>MiddleName</th>
        <th>LastName</th>
        <th>Birthday</th>
        <th>Gender</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Sity</th>
    </tr>
    {{#persons}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{middleName}}</td>
            <td>{{lastName}}</td>
            <td>{{birthday}}</td>
            <td>{{gender}}</td>
            <td>{{phone}}</td>
            <td>{{email}}</td>
            <td>{{sity}}</td>
        </tr>
    {{/persons}}
</table>
</body>
</html>